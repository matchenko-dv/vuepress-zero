    "contributors": [
    ]
  },
  
  "repository": {
  "type": "git",
  "url": "https://github.com/iwilfried/vuepress-zero.git"
  },

  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "clean": "rimraf docs/.vuepress/dist/*",
    "test": "rm -rf docs/.vuepress/dist && yarn docs:build && stat docs/.vuepress/dist/index.html"
  },
  
  "dependencies": {
    "markdown-it": "^8.4.1",
    "markdown-it-emoji": "^1.4.0",
    "markdown-it-katex": "^2.0.3"
  },
  
