{
  "name": "VuePress-Zero",
  "version": "0.0.1",
  "description": "The simplicity of this high quality documentation system is the beauty of VuePress.",
  "keywords": [
    "documentation",
    "vue",
    "vuepress",
    "generator"
  ],
  "homepage": "https://github.com/iwilfried/vuepress-zero#readme",
  
  "bugs": {
    "url": "https://github.com/iwilfried/vuepress-zero/issues"
  },
  "licence": "MIT",
  "author": {
    "name": "Wilfried Ifland",
    "email": "wilfried.iflandg@gmail.com",
    "url": "https://iwilfried.github.io/vuepress-zero/",
    "contributors": [
      "Brian Faust <brian@ark.io>"
    ]
  },
  
  "repository": {
  "type": "git",
  "url": "https://github.com/iwilfried/vuepress-zero.git"
  },

  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "clean": "rimraf docs/.vuepress/dist/*",
    "test": "rm -rf docs/.vuepress/dist && yarn docs:build && stat docs/.vuepress/dist/index.html",
    "now-build": "npm run docs:build && mv docs/.vuepress/dist dist"
  },
  
  "dependencies": {
    "markdown-it": "^8.4.1",
    "markdown-it-emoji": "^1.4.0",
    "markdown-it-katex": "^2.0.3",
    "vuetify": "^1.2.5"
  },
  
  "devDependencies": {
    "vuepress": "^0.14.8",
    "conventional-changelog": "^1.1.23",
    "eslint": "^4.19.1",
    "eslint-plugin-vue-libs": "^3.0.0",
    "vuepress-theme-vue": "^1.0.2",
    "yorkie": "^1.0.3",
    "highlight.js": "^9.12.0",
    "axios": "^0.18.0",
    "nprogress": "^0.2.0",
    "sweetalert": "^2.1.0"
  },
  "engines": {
    "node": ">=8"
  }
}



